  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'LicensePlist'
  pod 'NSURL+QueryDictionary'
  pod 'R.swift'
  pod 'SwiftLint'

end

target 'TodayExtension' do
  use_frameworks!

  pod 'AutoScrollLabel'

end
