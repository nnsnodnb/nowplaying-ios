target 'NowPlaying' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'AutoScrollLabel'
  pod 'Crashlytics'
  pod 'DTTJailbreakDetection'
  pod 'Fabric'
  pod 'Firebase/AdMob'
